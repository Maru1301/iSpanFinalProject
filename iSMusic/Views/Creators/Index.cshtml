@model IEnumerable<iSMusic.Models.EFModels.Creator>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.creatorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.creatorGender)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.creatorName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.creatorGender)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
        </td>
    </tr>
}

</table>
